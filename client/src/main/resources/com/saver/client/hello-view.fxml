<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.ListView?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.saver.client.HelloController">
    <HBox>
        <ListView fx:id="clientView" prefHeight="344.0" prefWidth="226.0"/>
        <ListView fx:id="serverView" prefHeight="344.0" prefWidth="226.0"/>
    </HBox>
    <HBox prefHeight="31.0" prefWidth="452.0">
        <Button onAction="#upload" mnemonicParsing="false" prefHeight="33.0" prefWidth="69.0" text="To server"/>
        <Button onAction="#download" mnemonicParsing="false" prefHeight="46.0" prefWidth="83.0" text="From server"/>
    </HBox>
</VBox>
